!-------------------------------------------------------------------------------
! fabm_nml
!-------------------------------------------------------------------------------
! models             [string array]
!                     a comma-separated list of one or model model names
!-------------------------------------------------------------------------------
&fabm_nml
   models = 'iow_age'
/

!-------------------------------------------------------------------------------
! iow_age - age tracer model
!-------------------------------------------------------------------------------
! inital_age [days]
! track_surface_age [logical]
!             set age at the surface to zero
! track_bottom_age [logical]
!             set age at the bottom to zero
!-------------------------------------------------------------------------------

&iow_age
   initial_age = 0.0,
   track_surface_age = .false.,
   track_bottom_age  = .false.,
/
